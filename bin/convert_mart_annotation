#!/usr/bin/env perl
package Bio::Deago::Main::ConvertMartAnnotation

# ABSTRACT: Convert BioMart annotation for use with deago
# PODNAME: convert_mart_annotation

=head1 SYNOPSIS

Collapses a TSV BioMart output file for use with deago

=cut

use Cwd qw(abs_path); 
BEGIN { unshift( @INC, abs_path('./lib') ) }
BEGIN { unshift( @INC, abs_path('./t/lib') ) }
use Bio::Deago::CommandLine::ConvertMartAnnotation;

Bio::Deago::CommandLine::ConvertMartAnnotation->new(args => \@ARGV, script_name => $0)->run;
