#!/usr/bin/env perl

package Bio::Deago::Main::DeagoMarkdownToHtml;

# ABSTRACT: Convert an R Markdown file into HTML report
# PODNAME: markdown

=head1 SYNOPSIS

Convert a tab-delimited annotation file (e.g. from BioMart) for use with deago.
=cut

BEGIN { unshift( @INC, '../lib' ) }
BEGIN { unshift( @INC, './lib' ) }
#BEGIN { unshift( @INC, '/software/pathogen/internal/prod/lib/' ) }
use Bio::Deago::CommandLine::DeagoMarkdownToHtml;

Bio::Deago::CommandLine::DeagoMarkdownToHtml->new(args => \@ARGV, script_name => $0)->run;